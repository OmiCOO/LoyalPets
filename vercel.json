{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/styles.css",
      "dest": "/src/public/styles.css"
    },
    {
      "src": "/auth.js",
      "dest": "/src/public/auth.js"
    },
    {
      "src": "/script.js",
      "dest": "/src/public/script.js"
    },
    {
      "src": "/admin.js",
      "dest": "/src/public/admin.js"
    },
    {
      "src": "/index.html",
      "dest": "/src/public/index.html"
    },
    {
      "src": "/signup.html",
      "dest": "/src/public/signup.html"
    },
    {
      "src": "/login.html",
      "dest": "/src/public/login.html"
    },
    {
      "src": "/",
      "dest": "/src/public/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/public/$1"
    }
  ]
}
